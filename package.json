{
  "name": "feedpile",
  "version": "1.0.0",
  "description": "rss reader",
  "main": "server/index.js",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "update-test": "npm run test -- -u",
    "test-coverage": "NODE_ENV=test jest --coverage",
    "build": "webpack",
    "start": "node server/index.js",
    "dev-client": "webpack-dev-server",
    "report-coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "deploy": "npm run build && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/captivators/feedpile.git"
  },
  "keywords": [
    "rss",
    "reader"
  ],
  "author": "DAFM",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/captivators/feedpile/issues"
  },
  "homepage": "https://github.com/captivators/feedpile#readme",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "client/**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 4,
        "functions": 21,
        "lines": 22,
        "statements": 14
      }
    }
  },
  "dependencies": {
    "auth0-lock": "^10.14.0",
    "axios": "^0.16.1",
    "body-parser": "^1.17.1",
    "chai": "^3.5.0",
    "coveralls": "^2.13.0",
    "cron": "^1.2.1",
    "dompurify": "^0.8.5",
    "express": "^4.15.2",
    "feedparser": "^2.1.0",
    "html-to-text": "^3.2.0",
    "iconv-lite": "^0.4.15",
    "json-loader": "^0.5.4",
    "jwt-decode": "^2.2.0",
    "material-ui": "^0.16.7",
    "material-ui-icons": "^1.0.0-alpha.2",
    "md5": "^2.2.1",
    "mocha": "^3.2.0",
    "moment": "^2.18.1",
    "mongoose": "^4.9.3",
    "morgan": "^1.8.1",
    "prop-types": "^15.5.8",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-saga": "^0.14.6",
    "request": "^2.81.0",
    "reselect": "^3.0.0",
    "webpack": "^2.3.3"
  },
  "devDependencies": {
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "dotenv": "^4.0.0",
    "enzyme": "^2.8.0",
    "enzyme-to-json": "^1.5.0",
    "jest": "^19.0.2",
    "sinon": "^2.1.0",
    "css-loader": "^0.28.0",
    "ghooks": "^2.0.0",
    "identity-obj-proxy": "^3.0.0",
    "react-hot-loader": "^1.3.1",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack-dev-server": "^2.4.2"
  },
  "config": {
    "ghooks": {
      "pre-push": "npm run test"
    }
  }
}
